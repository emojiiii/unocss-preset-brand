{
  "name": "unocss-preset-brand",
  "version": "1.0.0",
  "description": "The brand preset for Unocss",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/emojiiii/unocss-preset-brand/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/emojiiii/unocss-preset-brand/issues"
  },
  "repository": {
